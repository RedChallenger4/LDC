<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="app.theducksneezes.com.ldc.WatchMoviesMainMenuActivity">

    <Button
        android:id="@+id/myVideosButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="118dp"
        android:onClick="openToMyVideos"
        android:text="My Videos"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="97dp" />

    <Button
        android:id="@+id/netflixButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/myVideosButton"
        android:layout_alignRight="@+id/myVideosButton"
        android:layout_below="@+id/myVideosButton"
        android:layout_marginTop="117dp"
        android:onClick="openToNetflix"
        android:text="Netflix" />

</RelativeLayout>
